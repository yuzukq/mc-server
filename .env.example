# Cloudflare R2 設定
# このファイルを .env にコピーし、実際の値を入力してください

# Cloudflare アカウント ID
R2_ACCOUNT_ID=your_account_id_here

# R2 API トークン認証情報 (Cloudflare ダッシュボード > R2 > R2 API トークンの管理 で作成)
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here

# R2 バケット名 (Minecraft のワールドデータ用に作成したバケット)
R2_BUCKET_NAME=your_bucket_name_here

# R2 エンドポイント URL (形式: https://<account_id>.r2.cloudflarestorage.com)
R2_ENDPOINT=https://your_account_id_here.r2.cloudflarestorage.com

# ローカルデータディレクトリ (プロジェクトルートからの相対パス)
LOCAL_DATA_DIR=./server001/data

# ワールド名 (デフォルト: world)
WORLD_NAME=world
